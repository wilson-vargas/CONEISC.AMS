<ApplicationInsights xmlns="http://schemas.microsoft.com/ApplicationInsights/2013/Settings" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <InstrumentationKey xdt:Transform="Remove" xdt:Locator="Condition(normalize-space(text())='')"></InstrumentationKey>
  
  <ContextInitializers>
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.ComponentContextInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.DeviceContextInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.UserContextInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.SessionContextInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
  </ContextInitializers>
  <ContextInitializers xdt:Transform="Remove" xdt:Locator="Condition(count(*)=0)"/>

  <TelemetryInitializers>
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Core.TimestampPropertyInitializer, Microsoft.ApplicationInsights, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.NetBiosMachineNameTelemetryInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.OperatingSystemTelemetryInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.ProcessIdTelemetryInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.ProcessNameTelemetryInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.ThreadIdTelemetryInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.ThreadLanguageTelemetryInitializer, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
  </TelemetryInitializers>
  <TelemetryInitializers xdt:Transform="Remove" xdt:Locator="Condition(count(*)=0)"/>

  <TelemetryModules>
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.PageViewTelemetryModule, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
    <Add xdt:Transform="Remove" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Platform.UnhandledExceptionTelemetryModule, Microsoft.ApplicationInsights.Platform, ContentType=WindowsRuntime" />
  </TelemetryModules>
  <TelemetryModules xdt:Transform="Remove" xdt:Locator="Condition(count(*)=0)"/>

  <TelemetryChannel>
    <DeveloperMode xdt:Transform="Remove">false</DeveloperMode>
  </TelemetryChannel>
  <TelemetryChannel xdt:Transform="Remove" xdt:Locator="Condition(count(*)=0)"/>

</ApplicationInsights>